{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "product_meta": {
          "type": "product_meta",
          "settings": {
            "show_vendor": true,
            "show_sku": true,
            "show_reviews_badge": true,
            "show_share_buttons": true
          }
        },
        "variant_selector": {
          "type": "variant_selector",
          "settings": {
            "selector_mode": "block",
            "color_mode": "variant_image",
            "show_quantity_selector": false,
            "show_taxes_included": false,
            "show_inventory_quantity": true,
            "low_inventory_threshold": 72
          }
        },
        "63509649-6bd6-4baf-a630-f439c0f5ba96": {
          "type": "button",
          "settings": {
            "text": "Presentation",
            "link": "{{ product.metafields.custom.pdf_link.value }}"
          }
        },
        "eee211df-17ce-45dc-bcc4-d3f0e200f21f": {
          "type": "store_pickup",
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "disabled": true,
          "settings": {
            "show_payment_button": false,
            "show_gift_card_recipient": true
          }
        },
        "description": {
          "type": "description",
          "settings": {
            "display_mode": "view_more"
          }
        },
        "c281c06a-e1d1-429c-acf1-d9fd6cd3b0a3": {
          "type": "content",
          "settings": {
            "title": "Delivery Information",
            "content": "{{ product.metafields.custom.delivery_information | metafield_tag }}",
            "page": "",
            "display_mode": "view_more"
          }
        },
        "1181262e-e068-47fd-b6de-9f98b0b09dce": {
          "type": "liquid",
          "settings": {
            "title": "Decoration",
            "liquid": "{% assign decoration_object = product.metafields.custom.decoration.value | json %}\n{% if decoration_object != blank %}\n<img src={{decoration_object | remove: \"\\\"}} class=\"img-fluid\" alt=\"Responsive image\">\n{% endif %}",
            "display_mode": "show_all"
          }
        },
        "e89d3d4a-a2bd-4fce-8072-e9fe3cb798f0": {
          "type": "liquid",
          "settings": {
            "title": "Supporting Documents",
            "liquid": "{% assign data =  product.metafields.custom.dynamic_pdf_files.value | json_parse %}\n\n{% for json_data in data %}\n  {% assign object = json_data | json_parse %}\n  {% assign spec_file_object = object.spec_sheet.value %}\n  <a href=\"{{spec_file_object.url}}\" targe=\"_blank\">\n    <div class=\"file-name\">\nSpec Sheet Document<\/div>\n  <\/a>\n\n{% assign measurement_file_object = object.product_measurements.value %}\n  <a href=\"{{measurement_file_object.url}}\" targe=\"_blank\">\n    <div class=\"file-name\">\nProduct measurements document<\/div>\n  <\/a>\n\n{% assign decoration_file_object = object.decoration_spec_sheet.value %}\n  <a href=\"{{decoration_file_object.url}}\" targe=\"_blank\">\n    <div class=\"file-name\">\nDecoration spec sheet document<\/div>\n  <\/a>\n{% endfor %}",
            "display_mode": "show_all"
          }
        },
        "00f9dcd5-4f13-499e-ba6b-9b3c21de8cc3": {
          "type": "content",
          "settings": {
            "title": "Care Guide",
            "content": "<p>{{ product.metafields.descriptors.care_guide | metafield_tag }}<\/p>",
            "page": "",
            "display_mode": "show_all"
          }
        },
        "73d3e6f4-67c0-4a21-9052-5bcc4a4bf1cb": {
          "type": "shipping_estimator",
          "settings": {
            "shipping_estimator_default_country": "United States",
            "show_refunds_policy": true
          }
        }
      },
      "block_order": [
        "product_meta",
        "variant_selector",
        "63509649-6bd6-4baf-a630-f439c0f5ba96",
        "eee211df-17ce-45dc-bcc4-d3f0e200f21f",
        "buy_buttons",
        "description",
        "c281c06a-e1d1-429c-acf1-d9fd6cd3b0a3",
        "1181262e-e068-47fd-b6de-9f98b0b09dce",
        "e89d3d4a-a2bd-4fce-8072-e9fe3cb798f0",
        "00f9dcd5-4f13-499e-ba6b-9b3c21de8cc3",
        "73d3e6f4-67c0-4a21-9052-5bcc4a4bf1cb"
      ],
      "settings": {
        "image_size": "medium",
        "enable_video_looping": true,
        "enable_image_zoom": true,
        "zoom_effect": "outside",
        "carousel_effect": "fade"
      }
    },
    "6740b724-c142-40af-8b4c-200304f37558": {
      "type": "image-with-text",
      "disabled": true,
      "settings": {
        "image": "shopify:\/\/shop_images\/9280-SldblackTB-1-BC3909SldblackTBModelFront-337W.jpg",
        "image_position": "left",
        "image_width": 20,
        "title": "Your title",
        "content": "<p>Introducing the Ultimate Black Hoodie: Elevate Your Wardrobe with Timeless Sophistication<\/p>",
        "button_text": "Our products",
        "button_link": "shopify:\/\/collections\/all"
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "You may also like",
        "show_quick_buy": false
      }
    },
    "recently-viewed-products": {
      "type": "recently-viewed-products",
      "settings": {
        "title": "Recently viewed",
        "show_quick_buy": false
      }
    }
  },
  "order": [
    "main",
    "6740b724-c142-40af-8b4c-200304f37558",
    "product-recommendations",
    "recently-viewed-products"
  ]
}